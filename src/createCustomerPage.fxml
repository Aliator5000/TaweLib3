<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="CreateCustomerPageController">
   <children>
      <VBox layoutX="172.0" layoutY="30.0" prefHeight="340.0" prefWidth="257.0" spacing="10.0">
         <children>
            <TextField fx:id="usernameField" promptText="Username" />
            <TextField fx:id="firstNameField" promptText="First Name" />
            <TextField fx:id="surnameField" promptText="Surname" />
            <TextField fx:id="phoneNumberField" promptText="Phone Number" />
            <TextField fx:id="addressLine1Field" promptText="Address Line 1" />
            <TextField fx:id="addressLine2Field" promptText="Address Line 2" />
            <TextField fx:id="townField" promptText="Town" />
            <TextField fx:id="cityField" promptText="City" />
            <TextField fx:id="postcodeField" promptText="Post Code" />
            <Button mnemonicParsing="false" onAction="#createButtonHandler" text="Create" />
         </children></VBox>
   </children>
</AnchorPane>
